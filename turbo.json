{
  "baseBranch": "origin/main",
  "globalDependencies": [".env"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "marketplace#build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**"]
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "prettier-fix": {
      "outputs": []
    },
    "dev": {
      "cache": true
    },
    "start": {
      "cache": true
    },
    "types": {
      "outputs": ["dist/**/*.d.ts"]
    }
  }
}
