    "clsx": "1.1.1",
    "connectkit": "^0.0.1",
    "core-js": "^3.24.1",
    "email-validator": "2.0.4",
    "ethers": "5.6.8",
    "ipfs-http-client": "56.0.3",
    "js-cookie": "3.0.1",
    "keen-slider": "6.6.14",
    "lodash.random": "3.2.0",
    "lodash.throttle": "4.1.1",
    "next": "12.2.0",
