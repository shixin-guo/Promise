    "@types/body-scroll-lock": "3.1.0",
    "@types/circular-dependency-plugin": "^5",
    "@types/js-cookie": "3.0.2",
    "@types/lodash.random": "3.2.7",
    "@types/lodash.throttle": "4.1.7",
    "@types/node": "17.0.45",
    "@types/react": "17.0.45",
    "eslint": "8.17.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "8.5.0",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-preset-env": "9.5.2",
    "prettier": "2.7.1",
    "typescript": "4.7.4"
  },
  "next-unused": {
    "alias": {
      "@lib/*": [
        "lib/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "lib",
      "pages"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  }
